package companyDetails;

import java.util.Scanner;

public class MyClass 
{
	public static void main(String[] args) 
	{
		
		int n1=4,i,fact1=1;
		int n2=5,fact2=1;
		int n3=7,fact3=1;
		
		for(i=0;i<=n1;i++)
			fact1=fact1*i;
		
		for(i=1;i<=n2;i++)
			fact2=fact2*i;

		for(i=1;i<=n3;i++)
			fact3=fact3*i;
		
		System.out.println(fact1);
		System.out.println(fact2);
	
	}
}
